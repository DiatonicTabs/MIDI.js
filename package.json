{
  "name": "midi",
  "version": "0.4.2",
  "license": "MIT",
  "description": "Library to assist in creating HTML5 MIDI apps.",
  "author": "Michael Deal",
  "contributors": [
    "Sergi Mansilla",
    "Daniel van der Meer",
    "Mohit Muthanna",
    "Pete Otaqui"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mudcube/MIDI.js.git"
  },
  "main": "lib/MIDI.js",
  "devDependencies": {
    "aliasify": "^2.1.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-preset-stage-3": "^6.22.0",
    "babelify": "^7.3.0",
    "budo": "^9.4.7",
    "chai": "^3.5.0",
    "cross-env": "^3.0.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.5.2",
    "gulp-if": "^1.2.5",
    "gulp-minify-css": "^1.1.6",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.2.0",
    "gulp-useref": "^1.2.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^3.0.2",
    "package-json-versionify": "^1.0.4",
    "rimraf": "^2.5.4",
    "rollupify": "^0.3.9",
    "uglifyify": "^3.0.4"
  },
  "dependencies": {
    "debug": "^2.6.1",
    "postinstall-build": "^2.1.3"
  },
  "scripts": {
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "postinstall": "postinstall-build lib \"npm run build\"",
    "devserver": "budo js/MIDI.js:build/MIDI.js --live -- -t package-json-versionify -t [ babelify --presets [ es2015 stage-3 ] ] --standalone MIDI",
    "old.build": "mkdir -p build && browserify -t package-json-versionify -t [ babelify --presets [ es2015 stage-3 ] ] -t dedebug -t uglifyify -t rollupify --standalone MIDI js/MIDI.js > build/MIDI.js"
  },
  "aliasify": {
    "aliases": {
      "debug": "./js/shims/debug.js"
    }
  }
}
